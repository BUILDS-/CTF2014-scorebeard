<table class="table table-hover">
    <tr>
        <th>Challenge</th>
        <th>Team</th>
        <th>Points</th>
    </tr>
    <% Solve.all.reverse.each do |score| %>
        <tr>
            <td><%= score.title %></td>
            <td><%= HTMLEntities.new.encode(score.team) %></td>
            <td><%= score.points %></td>
        </tr>
    <% end %>

</table> 
